    <record model="ir.sequence" id="hr_expense_travel_sequence">
      <field name="name">Travel Expense Sequence</field>
      <field name="code">hr.expense.travel.sequence</field>
      <field name="padding">5</field>
    </record>
</data>
</openerp>
